{
  "name": "file-structure-generator",
  "version": "0.1.0",
  "description": "File structure generator util",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --history-api-fallback --inline --progress",
    "build": "rimraf dist && webpack --bail --progress --profile",
    "test:unit": "karma start karma.conf.js",
    "test:lint": "eslint .",
    "test": "npm run test:lint && npm run test:unit",
    "precommit": "npm run test",
    "release:major": "npm run build && git add -f dist/ && standard-version -a --no-verify -m \"release: %s\" --release-as major",
    "release:minor": "npm run build && git add -f dist/ && standard-version -a --no-verify -m \"release: %s\" --release-as minor",
    "release:patch": "npm run build && git add -f dist/ && standard-version -a --no-verify -m \"release: %s\" --release-as patch",
    "release": "npm run release:patch",
    "generate": "node ./src/index.js",
    "install": "node ./src/install.js"
  },
  "author": "Mateusz Jeziorek",
  "license": "ISC",
  "bin": {
    "fsg": "./src/index.js"
  },
  "devDependencies": {
    "ajv": "^6.5.3",
    "babel-eslint": "^9.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chalk": "^2.4.1",
    "eslint": "^5.5.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "ps-grab": "^1.0.1",
    "standard-version": "^4.4.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6",
    "yargs": "^12.0.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "base64-inline-loader": "^1.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "escape-string-regexp": "^1.0.5",
    "fs-extra": "^7.0.0",
    "klaw": "^3.0.0",
    "lodash": "^4.17.10",
    "node-libs-browser": "^2.1.0",
    "rimraf": "^2.6.2",
    "through2": "^2.0.3",
    "webpack": "^4.17.1"
  }
}
